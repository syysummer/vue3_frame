    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "^2.5.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.0",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "normalize.css": "^8.0.1",
